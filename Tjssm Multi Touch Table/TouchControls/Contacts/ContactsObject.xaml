        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="9*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image x:Name="bt_contactsObj" HorizontalAlignment="Center" Height="54" VerticalAlignment="Center" Width="253" Source="/TouchControls;component/Resources/ContactsObject.png" Grid.Row="1" Margin="-1.334,0,1.334,0"/>
        <Button x:Name="bt_img" Content="Button" HorizontalAlignment="Left" Height="54" VerticalAlignment="Top" Width="72.334" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Opacity="0" Margin="-1.334,0,0,0" Grid.Row="1"/>
